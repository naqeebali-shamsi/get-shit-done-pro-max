{
  "name": "get-shit-done-cc",
  "version": "1.6.4",
  "description": "A meta-prompting, context engineering and spec-driven development system for Claude Code by TACHES.",
  "bin": {
    "get-shit-done-cc": "bin/install.js"
  },
  "files": [
    "bin",
    "commands",
    "get-shit-done",
    "agents",
    "hooks"
  ],
  "scripts": {
    "build:rlm": "node ./node_modules/typescript/bin/tsc -p tsconfig.json",
    "test:rlm": "node --test dist/rlm/**/*.test.js",
    "postinstall:rlm": "node -e \"const fs=require('fs');const path=require('path');const dest='src/rlm/chunking/grammars';if(!fs.existsSync(dest))fs.mkdirSync(dest,{recursive:true});['tree-sitter-javascript','tree-sitter-typescript'].forEach(pkg=>{const src=path.join('node_modules',pkg);if(fs.existsSync(src)){const wasm=fs.readdirSync(src).filter(f=>f.endsWith('.wasm'));wasm.forEach(f=>fs.copyFileSync(path.join(src,f),path.join(dest,f)))}});\""
  },
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "meta-prompting",
    "context-engineering",
    "spec-driven-development"
  ],
  "author": "TACHES",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glittercowboy/get-shit-done.git"
  },
  "engines": {
    "node": ">=16.7.0"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.16.2",
    "compromise": "^14.14.3",
    "ollama": "^0.6.0",
    "tree-sitter-javascript": "^0.23.0",
    "tree-sitter-typescript": "^0.23.0",
    "web-tree-sitter": "^0.26.3",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
